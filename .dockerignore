*.py[cod]
*$py.class
*.so
.Python
*.egg-info/
dist/
build/

# Virtual environments
venv/
env/
ENV/
.venv

# Testing
.pytest_cache/
.coverage
htmlcov/
.tox/
tests/
*_test.py
test_*.py

# Jupyter
.ipynb_checkpoints
*.ipynb

# IDEs
.vscode/
.idea/
*.swp
*.swo
.DS_Store

# Git
.git/
.gitignore
.gitattributes

# DVC
.dvc/
*.dvc
.dvcignore

# MLflow
mlruns/
mlartifacts/

# Documentation
docs/
*.md
README.md
LICENSE

